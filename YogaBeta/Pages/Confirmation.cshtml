@page "/Confirmation"
@model YogaBeta.Pages.ConfirmationModel
@{
    int? poseDur = TempData["PoseDuration"] as int?;
    int? prepDur = TempData["PrepDuration"] as int?;
    string shava = TempData["Shavasana"] as string;
    int? shavaDur = TempData["ShavasanaDuration"] as int?;
    int? classDur = TempData["ClassDuration"] as int?;
    string[] jsonPoses = TempData["jsonPoses"] as string[];

    poseDur = poseDur * 60;


}


@(await Html.RenderComponentAsync<Generator3>(RenderMode.ServerPrerendered, 
  new { PoseDuration = poseDur, PrepDuration = prepDur, Shavasana = shava, ShavasanaDuration = shavaDur, ClassDuration = classDur, JsonPoses = jsonPoses}))